{
  "name": "catala-website",
  "version": "1.1.0",
  "repository": "https://github.com/CatalaLang/catala-website",
  "scripts": {
    "build:styles": "tailwind build -i src/css/index.css -o src/css/tailwind.css -c ./src/css/tailwind.config.js",
    "build:styles:watch": "tailwind build -i src/css/index.css -o src/css/tailwind.css -c ./src/css/tailwind.config.js -w",
    "assets": "rsync -r ./local-assets/* assets && rsync -r ./node_modules/@catala-lang/catala-web-assets/assets/*  assets",
    "build": "rescript build -with-deps",
    "start": "rescript build -with-deps -w",
    "clean": "rescript clean",
    "webpack": "webpack -w",
    "webpack:production": "NODE_ENV=production webpack",
    "server": "webpack serve",
    "eslint": "npx eslint src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "yarn run assets",
    "prestart": "yarn run assets"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@catala-lang/french-law": "^0.8.3-b.3",
    "@catala-lang/rescript-catala": "^0.8.1-b.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@rescript/react": "^0.11.0",
    "@rjsf/core": "^4.2.2",
    "babel-loader": "^8.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "refmt": "^1.13.7-1",
    "rescript": "^10.1.4",
    "rescript-hash": "^1.5.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@catala-lang/catala-web-assets": "0.10.2",
    "@greenlabs/res-tailwindcss": "^0.1.12",
    "@rescript/core": "^0.3.1",
    "ansi_up": "^5.1.0",
    "autoprefixer": "^10.4.8",
    "browser-builtins": "^3.3.1",
    "browserfs": "^1.4.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.21.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "favicons": "^7.1.1",
    "favicons-webpack-plugin": "^6.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "parcel-bundler": "^1.3.1",
    "postcss": "^8.4.21",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^2.2.19",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1"
  }
}
